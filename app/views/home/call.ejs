<%- include('../partials/header') %>

<div class="contain">
    <div class="header-list-call">
        <h2>History Call</h2>
    </div>

    <div class="list-call">

        <% calls.forEach( call => { %>
            <div class="box-call">
                <div class="thumbnail">
                    <img src="/img/<%= call.profile %>" alt="">
                </div>
                <div class="content">
                    <h2 class="name-call"><%= call.name %></h2>
                    <span><%= call.info %></span>
                    <span class="date"><%= call.date %></span>
                </div>
            </div>
        <% }) %>

    </div>
</div>
<%- include('../partials/bottom-bar') %>

<%- include('../partials/footer') %>